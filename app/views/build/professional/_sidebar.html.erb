	  <td><span class="sidebar_features_price_text"></span></td></tr>
	  <tr><td>Ecommerce</td><td><span class="sidebar_ecomm_hours_text"></span></td>
	  <td><span class="sidebar_ecomm_price_text"></span></td></tr>
	  <tr><td>Planning & Discovery</td><td><span class="sidebar_planning_hours_text"></span></td>
	  <td><span class="sidebar_planning_price_text"></span></td></tr>
	  <tr><td>Quality Assurance</td><td><span class="sidebar_qa_hours_text"></span></td>
	  <td><span class="sidebar_qa_price_text"></span></td></tr>
