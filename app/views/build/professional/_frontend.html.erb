<div id="pro_build_fe">
<h1>Front End Coding</h1>
<table cellpadding="0" cellspacing="0">
  <tr>
      <th class="title"></th><th>Quantity</th><th>Design Hours</th><th>Total Cost</th>
  </tr>

  <% @front_end.each do |d| %>

	<tr>
	<td><%= d.name %></td>
	<td><input value="<%= get_value(d.name,'front_end') %>" type="text" class="hours" name="front_end_<%= d.name.gsub!(' ','_') %>_hours" /></td>
	<td><span class="hours_text"><%= d.hours %></span></td>
	<td><span class="price_text">$0.00</span></td>
	</tr>
	
  <% end %>
    
</table>
</div>
