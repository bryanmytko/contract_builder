<div id="show_professional" class="dashboard_data_container dashboard_professional<%= session_table('professional') %>">
<table id="dashboard_table" cellpadding="0" cellspacing="0">

	<tr>
	  <th>&nbsp;</th>
	  <th><a href="#" rel="sort" class="follow_up_date">Follow-up Date</a></th>
	  <th><a href="#" rel="sort" class="company">Contact Information</a></th>
	  <th><a href="#" rel="sort" class="last_mod">Last Modified</a></th>
	  <th><a href="#" rel="sort" class="next_step">Next Step</a></th><th>&nbsp;</th>
	</tr>

	<% if(@professional) @professional.each do |w| %>

	<tr>

		<td><%= link_to "",edit_build_path(:id => w.id), :class => "dashboard_edit" %></td>
		<td><%= w.updated_at.strftime("%B %d, %Y")  %></td>
		<td><p><%= w.company %></p>
				<p><%= w.name %></p>
				<p><%= w.address %></p>
				<p><%= w.phone %></p>
				<p><%= w.email %></p>
				<p><%= w.url %></p>
				<p><%= link_to 'edit', edit_contract_path(:id => w.id) %></p>
		</td>
		<td><%= w.updated_at.strftime("%B %d, %Y")  %></td>
		<td>Email Client</td>
		<td></td>
	</tr>
	
	<% end %>
	<% end %>
	
</table>

<div class="dashboard_button_container">
    <%= link_to "", new_contract_path(:type => 'professional'), :class => "start_new_button" %>
</div>

</div>
